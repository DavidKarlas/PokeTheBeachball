<?xml version="1.0" encoding="UTF-8"?>
<ExtensionModel>
	<Extension path="/MonoDevelop/Ide/Commands">
		<Command id="PokeTheBeachball.StartStopListening"
			defaultHandler="PokeTheBeachball.StartStopListeningHandler"
			_label="Start poking Beachball" />
	</Extension>
	<Extension path="/MonoDevelop/Ide/MainMenu/Help">
		<CommandItem id="PokeTheBeachball.StartStopListening" />
	</Extension>
	<Extension path="/MonoDevelop/Ide/GlobalOptionsDialog">
		<Section id="PokeTheBeachball"
			_label="Poke The Beachball"
			insertafter="VersionControl">
			<Section id="General"
				_label="General"
				fill="true"
				class="PokeTheBeachball.GlobalOptionsPanel" />
		</Section>
	</Extension>
</ExtensionModel>